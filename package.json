{
  "name": "kpanel",
  "version": "2.0.0",
  "description": "Modern hosting control panel optimized for VPS 1GB",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm run build:client",
    "build:client": "cd client && npm run build",
    "install:all": "npm install && cd client && npm install",
    "production:start": "NODE_ENV=production node server.js",
    "production:pm2": "NODE_ENV=production pm2 start ecosystem.config.js"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.2.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.14.5",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "eslint": "^8.42.0",
    "jest": "^29.5.0",
    "nodemon": "^3.1.10",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "hosting",
    "control-panel",
    "vps",
    "nginx",
    "php",
    "mysql",
    "linux",
    "directadmin-alternative"
  ],
  "author": "KPanel Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/herfaaljihad/kpanel.git"
  },
  "bugs": {
    "url": "https://github.com/herfaaljihad/kpanel/issues"
  },
  "homepage": "https://github.com/herfaaljihad/kpanel#readme"
}
