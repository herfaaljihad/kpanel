{
  "name": "kpanel",
  "version": "2.0.0",
  "description": "Modern hosting control panel optimized for VPS 1GB",
  "main": "production-server.js",
  "scripts": {
    "start": "node production-server.js",
    "dev": "nodemon production-server.js",
    "dev:legacy": "nodemon server.js",
    "test": "jest",
    "setup": "node scripts/setup.js",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "build": "npm run build:client",
    "build:client": "cd client && npm run build",
    "install:all": "npm install && cd client && npm install",
    "production:start": "NODE_ENV=production node production-server.js",
    "production:pm2": "NODE_ENV=production pm2 start ecosystem.config.js"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "archiver": "^5.3.1",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^7.0.1",
    "fs-extra": "^11.1.1",
    "helmet": "^6.2.0",
    "ini": "^4.1.1",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^2.1.35",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.14.5",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.9.3",
    "sharp": "^0.32.1",
    "socket.io": "^4.7.1",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.0",
    "validator": "^13.15.15",
    "winston": "^3.8.2",
    "yaml": "^2.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "eslint": "^8.42.0",
    "jest": "^29.5.0",
    "nodemon": "^3.1.10",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "hosting",
    "control-panel",
    "vps",
    "nginx",
    "php",
    "mysql",
    "linux",
    "directadmin-alternative"
  ],
  "author": "KPanel Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/herfaaljihad/kpanel.git"
  },
  "bugs": {
    "url": "https://github.com/herfaaljihad/kpanel/issues"
  },
  "homepage": "https://github.com/herfaaljihad/kpanel#readme"
}
